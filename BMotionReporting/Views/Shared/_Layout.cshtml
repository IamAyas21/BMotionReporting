<body>
    <nav class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0 shadow">
        <a class="navbar-brand col-md-3 col-lg-2 mr-0 px-3" href="#">BMotion Reporting</a>
        <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-toggle="collapse" data-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <input class="form-control form-control-dark w-100" type="text" placeholder="Search" aria-label="Search">
        @*<ul class="navbar-nav px-3">
            <li class="nav-item text-nowrap">
                <a class="nav-link" href="#">Sign out</a>
            </li>
        </ul>*@
    </nav>

    <div class="container-fluid">
        <div class="row">
            <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse" style="overflow-y:hidden">
                <div class="sidebar-sticky pt-3">
                    <ul class="nav flex-column">
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link active" href="#">
                                <img src="~/Assets/icon_home.png" width="20" height="20" />
                                <span data-feather="home" style="padding:5px"></span>
                                Home
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("Index","UserAdmin")">
                                <img src="~/Assets/icon_admin.png" width="20" height="20" />
                                <span data-feather="home" style="padding:5px"></span>
                                User Administrator
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("Index","Fuel")">
                                <img src="~/Assets/icon_fuel.png" width="20" height="20" />
                                <span data-feather="home" style="padding:5px"></span>
                                Fuel
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("Index","Users")">
                                <img src="~/Assets/icon_daftar_pengguna.png" width="20" height="20" />
                                <span data-feather="file" style="padding:5px"></span>
                                Daftar Pengguna
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("Index","Orders")">
                                <img src="~/Assets/icon_pengambilan_bbm.png" width="20" height="20" />
                                <span data-feather="shopping-cart" style="padding:5px"></span>
                                Pengambilan BBM
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("Activity","Orders")">
                                <img src="~/Assets/icon_order.png" width="20" height="20" />
                                <span data-feather="shopping-cart" style="padding:5px"></span>
                                Order Monitoring
                            </a>
                        </li>
                        <li class="nav-item"  style="margin:10px;">
                            <a class="nav-link" href="#">
                                <img src="~/Assets/icon_notifikasi.png" width="30" height="30" />
                                <span data-feather="users" style="padding:5px"></span>
                                Notifikasi
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("History","Orders")">
                                <img src="~/Assets/icon_riwayat_pengambilan_bbm.png" width="20" height="20" />
                                <span data-feather="bar-chart-2" style="padding:5px"></span>
                                Riwayat Pengambilan BBM
                            </a>
                        </li>
                        <li class="nav-item" style="margin:10px;">
                            <a class="nav-link" href="@Url.Action("Index","Info")">
                                <img src="~/Assets/icon_notifikasi.png" width="20" height="20" />
                                <span data-feather="bar-chart-2" style="padding:5px"></span>
                                Info
                            </a>
                        </li>
                    </ul>
                    <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
                        <a class="nav-link" href="@Url.Action("Logout","Account")">
                            <img src="~/Assets/icon_keluar.png" width="20" height="20" />
                            <span data-feather="bar-chart-2" style="padding:5px"></span>
                            Keluar
                        </a>
                    </h6>

                </div>
            </nav>

            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
                <div style="margin:5%">
                    @RenderBody()
                </div>
                
                @*<footer class="footer mt-auto py-3">
                    <div class="container">
                        <span class="text-muted">&copy;@DateTime.Now.Year - BMotion Reporting</span>
                    </div>
                </footer>*@ 
            </main>        
        </div>
    </div>
</body>
</html>
