    @RenderSection("scripts", required: false)

    <script src="~/Scripts/AdminLTE/jquery.min.js"></script>
    <script src="~/Scripts/JS/qrcode.min.js"></script>
    <script src="~/Scripts/JS/instascan.min.js"></script>
    <script src="~/Scripts/AdminLTE/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/AdminLTE/moment.min.js"></script>
    <script src="~/Scripts/AdminLTE/jquery.inputmask.bundle.min.js"></script>
    <script src="~/Scripts/AdminLTE/daterangepicker.js"></script>
    <script src="~/Scripts/AdminLTE/bootstrap-colorpicker.min.js"></script>
    <script src="~/Scripts/AdminLTE/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/Scripts/AdminLTE/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/AdminLTE/dataTables.bootstrap4.min.js"></script>
    <script src="~/Scripts/AdminLTE/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/AdminLTE/responsive.bootstrap4.min.js"></script>
    <script src="~/Scripts/AdminLTE/adminlte.js"></script>
    <script src="~/Scripts/bootstrap.bundle.js"></script>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

        .container {
            width: auto;
            max-width: 680px;
            padding: 0 15px;
        }

        .footer {
            background-color: #f5f5f5;
        }
    </style>
    <!-- Custom styles for this template -->
    <link href="~/Content/CSS/dashboard.css" rel="stylesheet">
